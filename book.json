{
    "title": "下载开龙跟着学",
    "author": "lutao",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-sharing",
        "splitter",
        "-lunr", 
        "-search",
        "search-plus",
        "theme-comscore",
        "expandable-chapters-small",
        "favicon",
        "iframely",
        "toggle-chapters"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
         "tbfed-pagefooter": {
            "copyright": "Copyright © 南京商软 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}